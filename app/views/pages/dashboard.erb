<link href="/css/dashboard.css" media="screen" rel="stylesheet" type="text/css" media="screen, projection" />

<%= partial :'partials/navbar' %>
<form action="/dashboard">
  <div class="search">
    <input class="search-input" name="q" type="search"
      data-provide="typeahead" placeholder="Search"
      autocomplete="on"
    />
  </div>
</form>
<div class="dashboard">
  <div class="reading_lists">
    <% @reading_lists.each do |reading_list| %>
      <%= partial :'partials/reading_list', locals: {reading_list: reading_list} %>
    <% end %>
  </div>
  <%= partial :'partials/sidebar' %>
</div>
